lfrac: open out/lfrac.png; watchman-make --make "stack build && stack exec" -p package.yaml 'lib/**/*.hs' src/LFrac.hs -t lfrac
fifteen: stack run fifteen -- -w 500 -o out/fifteen.png --fpu 15; and rm -f out/fifteen.mp4; and ffmpeg -r 60 -f image2 -i out/fifteen%0(ls -1 out/fifteen*.png | head -n 1 | tr -C -d '[:digit:]' | wc -c)d.png -vcodec libx264 -crf 15 -pix_fmt yuv420p out/fifteen.mp4; and rm out/fifteen*.png
